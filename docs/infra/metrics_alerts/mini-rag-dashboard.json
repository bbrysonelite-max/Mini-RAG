{
  "id": null,
  "uid": "mini_rag_observability",
  "title": "Mini-RAG Observability",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "tags": ["mini-rag", "observability"],
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "workspace",
        "type": "query",
        "label": "Workspace",
        "datasource": "Prometheus",
        "refresh": 1,
        "hide": 0,
        "multi": true,
        "includeAll": true,
        "definition": "label_values(workspace_quota_ratio, workspace_id)",
        "current": {
          "text": "All",
          "value": "$__all"
        }
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Ask Latency (p95)",
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(ask_request_latency_seconds_bucket[$__interval])) by (le))",
          "legendFormat": "p95",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(ask_request_latency_seconds_bucket[$__interval])) by (le))",
          "legendFormat": "p99",
          "refId": "B"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "right"
        }
      }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Ask Error Ratio",
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit"
        },
        "overrides": []
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "targets": [
        {
          "expr": "100 * sum(rate(ask_requests_total{outcome=\"error\"}[$__interval])) / sum(rate(ask_requests_total[$__interval]))",
          "legendFormat": "error %",
          "refId": "A"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "right"
        },
        "thresholds": {
          "mode": "line",
          "steps": [
            {"color": "green", "value": null},
            {"color": "orange", "value": 2},
            {"color": "red", "value": 5}
          ]
        }
      }
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Ingest Throughput (chunks/min)",
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "targets": [
        {
          "expr": "sum(rate(ingest_processed_chunks_total[$__interval])) by (source)",
          "legendFormat": "{{source}}",
          "refId": "A"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "Ingest Failure Rate",
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit"
        },
        "overrides": []
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
      "targets": [
        {
          "expr": "100 * sum(rate(ingest_operations_total{outcome=\"failure\"}[$__interval])) by (source) / sum(rate(ingest_operations_total[$__interval])) by (source)",
          "legendFormat": "{{source}}",
          "refId": "A"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 5,
      "type": "table",
      "title": "Quota Usage Heatmap",
      "datasource": "Prometheus",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
      "targets": [
        {
          "expr": "workspace_quota_ratio{metric=\"requests_per_day\", workspace_id=~\"$workspace\"}",
          "legendFormat": "{{workspace_id}}",
          "refId": "A"
        },
        {
          "expr": "workspace_quota_ratio{metric=\"chunk_limit\", workspace_id=~\"$workspace\"}",
          "legendFormat": "{{workspace_id}}",
          "refId": "B"
        }
      ],
      "options": {
        "showHeader": true
      }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Background Job Failures",
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
      "targets": [
        {
          "expr": "sum(rate(background_jobs_completed_total{status=\"failed\"}[$__interval])) by (name)",
          "legendFormat": "{{name}}",
          "refId": "A"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "External Dependency Errors",
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 24},
      "targets": [
        {
          "expr": "sum(rate(external_request_errors_total[$__interval])) by (service, operation)",
          "legendFormat": "{{service}} / {{operation}}",
          "refId": "A"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    }
  ]
}
